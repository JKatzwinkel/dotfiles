! 
! Lösung, um die Probleme mit Ctrl-ARROWS in vim lösen
URxvt.keysym.Control-Up:\033[1;5A
URxvt.keysym.Control-Down:\033[1;5B
URxvt.keysym.Control-Left:\033[1;5D
URxvt.keysym.Control-Right:\033[1;5C
!URxvt.keysym.Control-Left:\033[0D
!URxvt.keysym.Control-Right:\033[0C

! Configuration for urxvt-perls
URxvt.perl-ext-common:default,clipboard,url-select,keyboard-select,font-size
URxvt.perl-ext:      default,clipboard,url-select,keyboard-select
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true
URxvt.keysym.Control-u:           perl:url-select:select_next
! URxvt.keysym.M-Escape:      perl:keaboard-select:activate
URxvt.keysym.M-a:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
!URxvt.keysym.Control-a:           perl:keyboard-select:activate
URxvt.keysym.Control-S-c:           perl:clipboard:copy
URxvt.keysym.Control-S-v:           perl:clipboard:paste
!URxvt.keysym.Control-S-v:         perl:clipboard:paste_escaped

URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal

URxvt.iso14755: false
URxvt.iso14755_52: false

urxvt.print-pipe: "cat > /dev/null"

URxvt*keysym.Home: \033[7~
URxvt*keysym.End: \033[8~
